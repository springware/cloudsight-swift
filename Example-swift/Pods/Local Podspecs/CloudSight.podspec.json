{
  "name": "CloudSight",
  "version": "1.0",
  "summary": "CloudSight image recognition API interface in Swift",
  "description": "CloudSight is a simple web API for image recognition.  This library is\nan implementation in Swift for developing applications that leverage\nthe CloudSight image recognition API, and is derived from the CamFind iOS app.",
  "homepage": "https://github.com/springware/cloudsight-swift.git",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Oasis": "brad@cloudsightapi.com"
  },
  "social_media_url": "http://twitter.com/CloudSightAPI",
  "source": {
    "git": "https://github.com/springware/cloudsight-swift.git",
    "tag": "1.0"
  },
  "frameworks": "UIKit",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "CloudSight/*.swift",
  "preserve_paths": "CocoaPods/**/*",
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS[sdk=macosx*]": "${PODS_ROOT}/CloudSight/CocoaPods/macosx",
    "SWIFT_INCLUDE_PATHS[sdk=iphoneos*]": "$(PODS_ROOT)/CloudSight/CocoaPods/iphoneos",
    "SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*]": "$(PODS_ROOT)/CloudSight/CocoaPods/iphonesimulator",
    "SWIFT_INCLUDE_PATHS[sdk=appletvos*]": "$(PODS_ROOT)/CloudSight/CocoaPods/appletvos",
    "SWIFT_INCLUDE_PATHS[sdk=appletvsimulator*]": "$(PODS_ROOT)/CloudSight/CocoaPods/appletvsimulator",
    "SWIFT_INCLUDE_PATHS[sdk=watchos*]": "$(PODS_ROOT)/CloudSight/CocoaPods/watchos",
    "SWIFT_INCLUDE_PATHS[sdk=watchsimulator*]": "$(PODS_ROOT)/CloudSight/CocoaPods/watchsimulator"
  }
}
